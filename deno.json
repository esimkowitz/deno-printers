{
  "name": "@printers/printers",
  "version": "0.3.12",
  "license": "MIT",
  "exports": {
    ".": "./index.ts"
  },
  "publish": {
    "include": [
      "*.ts",
      "README.md",
      "LICENSE",
      "napi/**",
      "target/release/*.dll",
      "target/release/*.dylib",
      "target/release/*.so"
    ],
    "exclude": [
      "!napi",
      "!target/release"
    ]
  },
  "tasks": {
    "dev": "deno run --allow-ffi --watch deno.ts",
    "build": "cargo build --release",
    "build:napi": "npm run build",
    "build:all": "deno run --allow-run --allow-read --allow-env scripts/build-all.ts",
    "test": "PRINTERS_JS_SIMULATE=true deno test --allow-ffi --allow-env --no-check tests/shared.test.ts",
    "test:real": "deno test --allow-ffi --allow-env tests/shared.test.ts",
    "test:doc": "deno test --doc --allow-ffi --allow-env deno.ts",
    "test:shared": "PRINTERS_JS_SIMULATE=true deno run --allow-ffi --allow-env tests/shared.test.ts",
    "test:node": "PRINTERS_JS_SIMULATE=true npx tsx tests/node-test-runner.ts",
    "test:bun": "PRINTERS_JS_SIMULATE=true bun test tests/shared.test.ts",
    "test:all": "PRINTERS_JS_SIMULATE=true deno run --allow-run --allow-write --allow-read --allow-env scripts/test-all.ts",
    "test:junit": "PRINTERS_JS_SIMULATE=true deno test --allow-ffi --allow-env tests/shared.test.ts --junit-path=test-results/deno-junit.xml",
    "test:all:real": "PRINTERS_JS_SIMULATE=false deno run --allow-run --allow-write --allow-read --allow-env scripts/test-all.ts",
    "dev:deno": "deno run --allow-ffi --allow-env --watch deno.ts",
    "dev:node": "npm run dev",
    "dev:bun": "bun --watch bun.js",
    "lint": "deno lint",
    "fmt": "deno fmt",
    "check": "deno check deno.ts",
    "check:all": "deno check deno.ts && npm run build && echo 'All runtimes checked successfully'",
    "bump:patch": "deno run -A scripts/bump-version.ts patch",
    "bump:minor": "deno run -A scripts/bump-version.ts minor",
    "bump:major": "deno run -A --allow-write scripts/bump-version.ts major",
    "docs": "deno doc --html --name=\"@printers/printers\" deno.ts index.ts",
    "docs:serve": "deno doc --serve --name=\"@printers/printers\" deno.ts index.ts"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "@std/path": "jsr:@std/path@1",
    "@std/semver": "jsr:@std/semver@1",
    "@cross/test": "jsr:@cross/test@0.0.10"
  },
  "fmt": {
    "exclude": [
      "target/",
      "napi/",
      "coverage/",
      "reports/",
      "test-results/",
      ".claude/",
      "LICENSE",
      "*.rs",
      "Cargo.{toml,lock}",
      "package.json",
      "node.ts",
      "bun.ts",
      "index.ts"
    ]
  },
  "lint": {
    "exclude": [
      "target/",
      "napi/",
      "coverage/",
      "reports/",
      "test-results/",
      ".claude/",
      "examples/",
      "*.rs",
      "Cargo.{toml,lock}",
      "package.json",
      "node.ts",
      "bun.ts",
      "index.ts",
      "tests/node-test-runner.ts",
      "scripts/move-napi-artifacts.ts",
      "scripts/remove-env-check.ts"
    ]
  },
  "exclude": [
    ".devcontainer/",
    ".vscode/",
    ".zed/",
    "target/",
    "napi/",
    "coverage/",
    "test-results/",
    "package.json",
    "tests/node-test-runner.ts",
    "scripts/move-napi-artifacts.js"
  ],
  "nodeModulesDir": "auto",
  "compilerOptions": {
    "lib": [
      "deno.window",
      "deno.unstable"
    ],
    "strict": true
  }
}
