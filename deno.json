{
  "name": "@printers/printers",
  "version": "0.3.16",
  "license": "MIT",
  "exports": {
    ".": "./index.ts"
  },
  "publish": {
    "include": [
      "*.ts",
      "README.md",
      "LICENSE",
      "npm/**",
      "target/release/*.dll",
      "target/release/*.dylib",
      "target/release/*.so"
    ],
    "exclude": [
      "!npm",
      "!target/release"
    ]
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "@std/path": "jsr:@std/path@1",
    "@std/semver": "jsr:@std/semver@1",
    "@cross/test": "jsr:@cross/test@0.0.10"
  },
  "fmt": {
    "exclude": [
      "target/",
      "npm/",
      "coverage/",
      "reports/",
      "test-results/",
      ".claude/",
      "LICENSE",
      "*.rs",
      "Cargo.{toml,lock}",
      "package.json",
      "node.ts",
      "bun.ts",
      "index.ts"
    ]
  },
  "lint": {
    "exclude": [
      "target/",
      "npm/",
      "coverage/",
      "reports/",
      "test-results/",
      ".claude/",
      "examples/",
      "*.rs",
      "Cargo.{toml,lock}",
      "package.json",
      "node.ts",
      "bun.ts",
      "index.ts",
      "tests/node-test-runner.ts",
      "scripts/*.js"
    ],
    "rules": {
      "exclude": ["ban-unused-ignore"]
    }
  },
  "exclude": [
    ".devcontainer/",
    ".vscode/",
    ".zed/",
    "target/",
    "npm/",
    "coverage/",
    "test-results/",
    "package.json",
    "tests/node-test-runner.ts",
    "scripts/*.js"
  ],
  "nodeModulesDir": "auto",
  "compilerOptions": {
    "lib": [
      "deno.window",
      "deno.unstable"
    ],
    "strict": true
  }
}
