{
  "name": "printers-example-node-jsr",
  "version": "1.0.0",
  "description": "Node.js JSR example for @printers/printers",
  "type": "module",
  "main": "main.js",
  "scripts": {
    "start": "node --experimental-strip-types main.js",
    "dev": "node --experimental-strip-types --watch main.js",
    "start:legacy": "node --experimental-modules --experimental-json-modules main.js"
  },
  "dependencies": {
    "@printers/printers": "npm:@jsr/printers__printers@^0.4"
  },
  "engines": {
    "node": ">= 22"
  },
  "license": "MIT"
}
