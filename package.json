{
  "name": "@printers/printers",
  "version": "0.3.16",
  "description": "Cross-platform printer library for Node.js, Deno, and Bun",
  "type": "module",
  "main": "index.ts",
  "files": [
    "index.ts",
    "node.ts",
    "deno.ts",
    "bun.ts",
    "target/release/*.dll",
    "target/release/*.dylib",
    "target/release/*.so"
  ],
  "napi": {
    "binaryName": "printers",
    "targets": [
      "x86_64-apple-darwin",
      "aarch64-apple-darwin",
      "x86_64-pc-windows-msvc",
      "aarch64-pc-windows-msvc",
      "x86_64-unknown-linux-gnu",
      "aarch64-unknown-linux-gnu"
    ]
  },
  "engines": {
    "node": ">= 20"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/esimkowitz/printers-js.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@cross/test": "npm:@jsr/cross__test@^0.0.10",
    "@napi-rs/cli": "^3.1.5",
    "@std/assert": "npm:@jsr/std__assert@^1.0.14",
    "@types/bun": "^1.2.21",
    "@types/node": "^24.3.1",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "c8": "^10.1.2",
    "eslint": "^9.35.0",
    "tsx": "^4.20.5"
  },
  "scripts": {
    "test": "PRINTERS_JS_SIMULATE=true npx tsx tests/node-test-runner.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "napi prepublish -t npm --gh-release false --dry-run"
  },
  "packageManager": "npm@10.0.0"
}
